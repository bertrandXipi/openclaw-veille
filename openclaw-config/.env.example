# OpenClaw Environment Variables
# Copy this file to .env and fill in the values

# Gemini API Key (required)
GEMINI_API_KEY=AIza...

# Second-brain API URL
SECOND_BRAIN_API_URL=http://localhost:3100

# WhatsApp phone number (with country code)
MY_PHONE_NUMBER=+33612345678

# Gateway token (generated during setup)
OPENCLAW_GATEWAY_TOKEN=your-secure-token-here

# Optional: NotebookLM MCP URL (if using NotebookLM MCP server)
NOTEBOOKLM_MCP_URL=http://127.0.0.1:8000/mcp
